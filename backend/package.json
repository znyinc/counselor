{
  "name": "ai-career-counseling-backend",
  "version": "1.0.0",
  "description": "Backend API for AI Career Counseling Tool",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "start:production": "NODE_ENV=production node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "build:production": "NODE_ENV=production tsc",
    "test": "jest",
    "test:integration": "jest --testPathPattern=\"integration\"",
    "test:unit": "jest --testPathPattern=\"__tests__\" --testPathIgnorePatterns=\"integration\"",
    "test:performance": "jest --testPathPattern=\"performance\"",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "cache:clear": "node -e \"console.log('Cache cleared')\"",
    "performance:profile": "node --prof dist/index.js",
    "performance:analyze": "node --prof-process isolate-*.log > profile.txt"
  },
  "dependencies": {
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "openai": "^4.20.1",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/uuid": "^9.0.7",
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  },
  "keywords": ["api", "career-counseling", "ai", "education"],
  "author": "Ashish Ranjan Tiwary",
  "license": "MIT"
}