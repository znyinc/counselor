# Production Environment Configuration for Frontend
REACT_APP_API_URL=https://api.your-domain.com
REACT_APP_ENVIRONMENT=production

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_OFFLINE_MODE=true
REACT_APP_ENABLE_PWA=true

# Performance Configuration
REACT_APP_ENABLE_SERVICE_WORKER=true
REACT_APP_CACHE_STATIC_ASSETS=true

# Analytics Configuration (if using Google Analytics, etc.)
REACT_APP_GA_TRACKING_ID=your_ga_tracking_id_here

# Error Reporting (if using Sentry, etc.)
REACT_APP_SENTRY_DSN=your_sentry_dsn_here

# Build Configuration
GENERATE_SOURCEMAP=false
INLINE_RUNTIME_CHUNK=false

# Bundle Analysis
REACT_APP_BUNDLE_ANALYZER=false